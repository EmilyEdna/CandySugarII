        x:Key="Sexlab"
        FontFamily="Solid"
        Glyph="{Static lib:FontIcon.Venus}"
        Size="22"
        Color="{StaticResource Red}" />
    <FontImage
        x:Key="Manga"
        FontFamily="Solid"
        Glyph="{Static lib:FontIcon.Citrus}"
        Size="22"
        Color="{StaticResource Red}" />
    <FontImage
        x:Key="Image"
        FontFamily="Solid"
        Glyph="{Static lib:FontIcon.Strawberry}"
        Size="22"
        Color="{StaticResource Red}" />
    <FontImage
        x:Key="Music"
        FontFamily="Solid"
        Glyph="{Static lib:FontIcon.Music}"
        Size="22"
        Color="{StaticResource Red}" />
