        x:Key="Down"
        FontFamily="Solid"
        Glyph="{Static lib:FontIcon.Down}"
        Size="15"
        Color="{StaticResource Primary}" />

    <FontImage
        x:Key="Play"
        FontFamily="Solid"
        Glyph="{Static lib:FontIcon.Play}"
        Size="12"
        Color="{StaticResource Red}" />
    <FontImage
        x:Key="Album"
        FontFamily="Solid"
        Glyph="{Static lib:FontIcon.RecordVinyl}"
        Size="45"
        Color="{StaticResource Black}" />

    <FontImage
        x:Key="Ad"
        FontFamily="Thin"
        Glyph="{Static lib:FontIcon.RectangleAd}"
        Size="15"
        Color="{StaticResource Primary}" />
    <FontImage
        x:Key="Re"
        FontFamily="Thin"
        Glyph="{Static lib:FontIcon.ArrowRotateRight}"
        Size="15"
        Color="{StaticResource Primary}" />
