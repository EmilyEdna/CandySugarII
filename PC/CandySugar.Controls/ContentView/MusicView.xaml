                                                                    <MultiBinding Converter="{StaticResource ToMulti}">
                                                                        <Binding Path="Tag" RelativeSource="{RelativeSource AncestorType=TextBlock, Mode=FindAncestor}" />
                                                                        <Binding Path="SongId" />
                                                                    </MultiBinding>
                                                                </i:InvokeCommandAction.CommandParameter>
                                                            </i:InvokeCommandAction>
                                                        </i:EventTrigger>
                                                    </i:Interaction.Triggers>
