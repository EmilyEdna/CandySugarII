                                <ListBox.ItemTemplate>
                                    <DataTemplate>
                                        <Grid
                                            Margin="0,0,0,5"
                                            HorizontalAlignment="Left"
                                            Cursor="Hand">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width=".3*" />
                                                <ColumnDefinition Width=".7*" />
                                            </Grid.ColumnDefinitions>
                                            <Border>
                                                <Image
                                                    Width="80"
                                                    Height="80"
                                                    SnapsToDevicePixels="True"
                                                    Source="{Binding Cover}"
                                                    Stretch="Uniform" />
                                                <Border.Effect>
                                                    <DropShadowEffect
                                                        BlurRadius="6"
                                                        Opacity="1"
                                                        ShadowDepth="0"
                                                        Color="{DynamicResource SugarThemePinkColor}" />
                                                </Border.Effect>
                                            </Border>
                                            <StackPanel Grid.Column="1" Margin="2,0,0,0">
                                                <TextBlock
                                                    FontSize="12"
                                                    Foreground="{DynamicResource SugarThemeSmoke}"
                                                    Text="{Binding SongSheetName, StringFormat=歌单名称:{0}}"
                                                    TextWrapping="WrapWithOverflow" />
                                                <TextBlock
                                                    Margin="0,10,0,0"
                                                    FontSize="10"
                                                    Foreground="{DynamicResource SugarThemeRed}"
                                                    Text="{Binding ListenNumber, StringFormat=播放次数:{0}}" />
                                                <TextBlock
                                                    Margin="0,10,0,0"
                                                    FontSize="10"
                                                    Foreground="{DynamicResource SugarThemeRed}"
                                                    Text="{Binding CreateTime, StringFormat=创建时间:{0:yyyy-MM-dd}}" />
                                                <TextBlock
                                                    Margin="0,10,0,0"
                                                    FontSize="10"
                                                    Foreground="{DynamicResource SugarThemeBlue}"
                                                    MouseUp="MouseUpChanged"
                                                    Text="查看详情">
                                                    <i:Interaction.Triggers>
                                                        <i:EventTrigger EventName="MouseDown">
                                                            <i:InvokeCommandAction
                                                                s:View.ActionTarget="{Binding DataContext, ElementName=MusicViewName}"
                                                                Command="{s:Action ShowSheetAction}"
                                                                CommandParameter="{Binding SongSheetId}" />
                                                        </i:EventTrigger>
                                                    </i:Interaction.Triggers>
                                                </TextBlock>
                                            </StackPanel>
                                        </Grid>
                                    </DataTemplate>
                                </ListBox.ItemTemplate>
                            </ListBox>
                            <hc:Pagination
                                Grid.Row="1"
                                HorizontalAlignment="Center"
                                IsJumpEnabled="False"
                                MaxPageCount="{Binding Total}"
                                Style="{StaticResource PageinationStyle}"
                                PageIndex="{Binding Page}">
                                <hc:Interaction.Triggers>
                                    <hc:EventTrigger EventName="PageUpdated">
                                        <hc:EventToCommand
                                            s:View.ActionTarget="{Binding DataContext, ElementName=MusicViewName}"
                                            Command="{s:Action PageAction}"
                                            PassEventArgsToCommand="True" />
                                    </hc:EventTrigger>
                                </hc:Interaction.Triggers>
                            </hc:Pagination>
                        </Grid>
                    </Grid>
                    <!--  歌单详情  -->
                    <Grid
                        x:Name="详情"
                        RenderTransformOrigin="1,0.5"
                        Visibility="Collapsed">
                        <Grid.RenderTransform>
                            <TransformGroup>
                                <TranslateTransform />
                            </TransformGroup>
                        </Grid.RenderTransform>
                        <Rectangle
                            Height="{c:Binding (ScreenHeight*0.75),
                                               Source={x:Static res:CandySoft.Default}}"
                            Margin="10,90,0,0"
                            StrokeThickness="0">
                            <Rectangle.Fill>
                                <SolidColorBrush Opacity="0.4" Color="White" />
                            </Rectangle.Fill>
                            <Rectangle.Effect>
                                <DropShadowEffect
                                    BlurRadius="15"
                                    Direction="300"
                                    Opacity="0.3" />
                            </Rectangle.Effect>
                        </Rectangle>
                        <Grid Margin="10,90,0,0">
                            <WrapPanel Orientation="Vertical">
                                <Grid HorizontalAlignment="Left" VerticalAlignment="Top">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition />
                                        <ColumnDefinition />
                                    </Grid.ColumnDefinitions>
                                    <Border Margin="1,1,0,0">
                                        <Image
                                            Width="80"
                                            Height="80"
                                            SnapsToDevicePixels="True"
                                            Source="{Binding DetailResult.Logo}"
                                            Stretch="Uniform" />
                                        <Border.Effect>
                                            <DropShadowEffect
                                                BlurRadius="6"
                                                Opacity="1"
                                                ShadowDepth="0"
                                                Color="{DynamicResource SugarThemePinkColor}" />
                                        </Border.Effect>
                                    </Border>
                                    <StackPanel Grid.Column="1" Margin="10,0,0,0">
                                        <TextBlock
                                            FontSize="12"
                                            Foreground="{DynamicResource SugarThemeSmoke}"
                                            Text="{Binding DetailResult.DissName, StringFormat=歌单名称:{0}}"
                                            TextWrapping="WrapWithOverflow" />
                                        <TextBlock
                                            Margin="0,10,0,0"
                                            FontSize="12"
                                            Foreground="{DynamicResource SugarThemeRed}"
                                            Text="{Binding DetailResult.MusicNum, StringFormat=歌曲数量:{0}}"
                                            TextWrapping="WrapWithOverflow" />
                                        <TextBlock
                                            Margin="0,10,0,0"
                                            FontSize="10"
                                            Foreground="{DynamicResource SugarThemeRed}"
                                            Text="{Binding DetailResult.ListenNum, StringFormat=播放次数:{0}}" />
                                        <TextBlock
                                            Margin="0,10,0,0"
                                            FontSize="10"
                                            Foreground="{DynamicResource SugarThemeRed}"
                                            Text="{Binding DetailResult.CreateTime, StringFormat=创建时间:{0:yyyy-MM-dd}}" />
                                    </StackPanel>
                                </Grid>
                                <UniformGrid HorizontalAlignment="Center">
                                    <hc:Divider
                                        Width="{Binding ScreenWidth, Source={x:Static res:CandySoft.Default}}"
                                        Margin="0,3,0,0"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        Content="详情列表"
                                        Foreground="{DynamicResource SugarThemeWhite}"
                                        LineStroke="{DynamicResource SugarThemeWhite}" />
                                </UniformGrid>
                            </WrapPanel>
                            <ListBox
                                Margin="0,95,0,0"
                                Background="Transparent"
                                BorderThickness="0"
                                ItemsSource="{Binding DetailResult.ElementResults}"
                                Style="{StaticResource WrapPanelVerticalListBox}">
                                <ListBox.ItemsPanel>
                                    <ItemsPanelTemplate>
                                        <UniformGrid Columns="1" />
                                    </ItemsPanelTemplate>
                                </ListBox.ItemsPanel>
                                <ListBox.ItemTemplate>
                                    <DataTemplate>
                                        <StackPanel Margin="2,0,0,10">
                                            <UniformGrid Columns="2">
                                                <TextBlock
                                                    FontSize="12"
                                                    Foreground="{DynamicResource SugarThemeSmoke}"
                                                    Text="{Binding SongName}" />
                                                <TextBlock
                                                    HorizontalAlignment="Right"
                                                    Style="{StaticResource Common}"
                                                    Tag="Detail">
                                                    <md:PackIcon Margin="0,0,5,0" Kind="PlayCircleOutline" />
                                                    <i:Interaction.Triggers>
                                                        <i:EventTrigger EventName="MouseDown">
                                                            <i:InvokeCommandAction s:View.ActionTarget="{Binding DataContext, ElementName=MusicViewName}" Command="{s:Action PlayListAction}">
                                                                <i:InvokeCommandAction.CommandParameter>
                                                                    <MultiBinding Converter="{StaticResource ToMulti}">
                                                                        <Binding Path="Tag" RelativeSource="{RelativeSource AncestorType=TextBlock, Mode=FindAncestor}" />
                                                                        <Binding Path="SongId" />
                                                                    </MultiBinding>
                                                                </i:InvokeCommandAction.CommandParameter>
                                                            </i:InvokeCommandAction>
                                                        </i:EventTrigger>
                                                    </i:Interaction.Triggers>
                                                </TextBlock>
                                            </UniformGrid>
                                            <TextBlock
                                                Margin="0,10,0,0"
                                                FontSize="10"
                                                Foreground="{DynamicResource SugarThemeRed}"
                                                Text="{Binding SongArtistName, Converter={StaticResource ToChar}, StringFormat=歌手:({0})}" />
                                            <TextBlock
                                                Margin="0,10,0,0"
                                                Cursor="Hand"
                                                FontSize="10"
                                                Foreground="{DynamicResource SugarThemeRed}"
                                                MouseUp="MouseUpChanged"
                                                Text="{Binding SongAlbumName, StringFormat=专辑:({0})}"
                                                TextWrapping="WrapWithOverflow">
                                                <i:Interaction.Triggers>
                                                    <i:EventTrigger EventName="MouseDown">
                                                        <i:InvokeCommandAction
                                                            s:View.ActionTarget="{Binding DataContext, ElementName=MusicViewName}"
                                                            Command="{s:Action LinkAlbumAction}"
                                                            CommandParameter="{Binding SongAlbumId}" />
                                                    </i:EventTrigger>
                                                </i:Interaction.Triggers>
                                            </TextBlock>
                                        </StackPanel>
                                    </DataTemplate>
                                </ListBox.ItemTemplate>
                            </ListBox>
                        </Grid>
                    </Grid>
                    <!--  专辑  -->
                    <Grid
                        x:Name="专辑"
                        RenderTransformOrigin="1,0.5"
                        Visibility="Collapsed">
                        <Grid.RenderTransform>
                            <TransformGroup>
                                <TranslateTransform />
                            </TransformGroup>
                        </Grid.RenderTransform>
                        <Rectangle
                            Height="{c:Binding (ScreenHeight*0.75),
                                               Source={x:Static res:CandySoft.Default}}"
                            Margin="10,90,0,0"
                            StrokeThickness="0">
                            <Rectangle.Fill>
                                <SolidColorBrush Opacity="0.4" Color="White" />
                            </Rectangle.Fill>
                            <Rectangle.Effect>
                                <DropShadowEffect
                                    BlurRadius="15"
                                    Direction="300"
                                    Opacity="0.3" />
                            </Rectangle.Effect>
                        </Rectangle>
                        <ListBox
                            Margin="10,90,0,0"
                            Background="Transparent"
                            BorderThickness="0"
                            ItemsSource="{Binding AlbumResult}"
                            Style="{StaticResource WrapPanelVerticalListBox}">
                            <ListBox.ItemsPanel>
                                <ItemsPanelTemplate>
                                    <UniformGrid Columns="1" />
                                </ItemsPanelTemplate>
                            </ListBox.ItemsPanel>
                            <ListBox.ItemTemplate>
                                <DataTemplate>
                                    <StackPanel Margin="2,0,0,10">
                                        <TextBlock
                                            FontSize="12"
                                            Foreground="{DynamicResource SugarThemeSmoke}"
                                            Text="{Binding SongName}" />
                                        <UniformGrid Columns="2">
                                            <TextBlock
                                                Margin="0,10,0,0"
                                                FontSize="10"
                                                Foreground="{DynamicResource SugarThemeRed}"
                                                Text="{Binding SongArtistName, Converter={StaticResource ToChar}, StringFormat=歌手:({0})}" />
                                            <TextBlock
                                                HorizontalAlignment="Right"
                                                Style="{StaticResource Common}"
                                                Tag="Album">
                                                <md:PackIcon Margin="0,0,5,0" Kind="PlayCircleOutline" />
                                                <i:Interaction.Triggers>
                                                    <i:EventTrigger EventName="MouseDown">
                                                        <i:InvokeCommandAction s:View.ActionTarget="{Binding DataContext, ElementName=MusicViewName}" Command="{s:Action PlayListAction}">
                                                            <i:InvokeCommandAction.CommandParameter>
                                                                <MultiBinding Converter="{StaticResource ToMulti}">
                                                                    <Binding Path="Tag" RelativeSource="{RelativeSource AncestorType=TextBlock, Mode=FindAncestor}" />
                                                                    <Binding Path="SongId" />
                                                                </MultiBinding>
                                                            </i:InvokeCommandAction.CommandParameter>
                                                        </i:InvokeCommandAction>
                                                    </i:EventTrigger>
                                                </i:Interaction.Triggers>
                                            </TextBlock>
                                        </UniformGrid>
                                        <TextBlock
                                            Margin="0,10,0,0"
                                            Cursor="Hand"
                                            FontSize="10"
                                            Foreground="{DynamicResource SugarThemeRed}"
                                            Text="{Binding SongAlbumName, StringFormat=专辑:({0})}"
                                            TextWrapping="WrapWithOverflow" />
                                    </StackPanel>
                                </DataTemplate>
                            </ListBox.ItemTemplate>
                        </ListBox>
                    </Grid>
                    <!--  列表  -->
                    <Grid
                        x:Name="列表"
                        RenderTransformOrigin="1,0.5"
                        Visibility="Collapsed">
                        <Grid.RenderTransform>
                            <TransformGroup>
                                <TranslateTransform />
                            </TransformGroup>
                        </Grid.RenderTransform>
                        <Rectangle
                            Height="{c:Binding (ScreenHeight*0.75),
                                               Source={x:Static res:CandySoft.Default}}"
                            Margin="10,90,0,0"
                            StrokeThickness="0">
                            <Rectangle.Fill>
                                <SolidColorBrush Opacity="0.4" Color="White" />
                            </Rectangle.Fill>
                            <Rectangle.Effect>
                                <DropShadowEffect
                                    BlurRadius="15"
                                    Direction="300"
                                    Opacity="0.3" />
                            </Rectangle.Effect>
                        </Rectangle>
                        <ListBox
                            Margin="10,90,0,0"
                            Background="Transparent"
                            BorderThickness="0"
                            ItemsSource="{Binding CandyList}"
                            Style="{StaticResource WrapPanelVerticalListBox}">
                            <ListBox.ItemsPanel>
                                <ItemsPanelTemplate>
                                    <UniformGrid VerticalAlignment="Top" Columns="1" />
                                </ItemsPanelTemplate>
                            </ListBox.ItemsPanel>
                            <ListBox.ItemTemplate>
                                <DataTemplate>
                                    <StackPanel Margin="2,0,0,10">
                                        <TextBlock
                                            FontSize="12"
                                            Foreground="{DynamicResource SugarThemeSmoke}"
                                            Text="{Binding SongName}" />
                                        <UniformGrid Columns="2">
                                            <TextBlock
                                                Margin="0,10,0,0"
                                                FontSize="10"
                                                Foreground="{DynamicResource SugarThemeRed}"
                                                Text="{Binding SongArtist, Converter={StaticResource ToChar}, StringFormat=歌手:({0})}" />
                                            <TextBlock
                                                HorizontalAlignment="Right"
                                                Style="{StaticResource Common}"
                                                Tag="Album">
                                                <md:PackIcon Margin="0,0,5,0" Kind="DeleteOutline" />
                                                <i:Interaction.Triggers>
                                                    <i:EventTrigger EventName="MouseDown">
                                                        <i:InvokeCommandAction
                                                            s:View.ActionTarget="{Binding DataContext, ElementName=MusicViewName}"
                                                            Command="{s:Action RemoveAction}"
                                                            CommandParameter="{Binding CandyId}" />
                                                    </i:EventTrigger>
                                                </i:Interaction.Triggers>
                                            </TextBlock>
                                        </UniformGrid>
                                        <TextBlock
                                            Margin="0,10,0,0"
                                            Cursor="Hand"
                                            FontSize="10"
                                            Foreground="{DynamicResource SugarThemeRed}"
                                            MouseUp="MouseUpChanged"
                                            Text="{Binding AlbumName, StringFormat=专辑:({0})}"
                                            TextWrapping="WrapWithOverflow">
                                            <i:Interaction.Triggers>
                                                <i:EventTrigger EventName="MouseDown">
                                                    <i:InvokeCommandAction
                                                        s:View.ActionTarget="{Binding DataContext, ElementName=MusicViewName}"
                                                        Command="{s:Action AlbumAction}"
                                                        CommandParameter="{Binding AlbumId}" />
                                                </i:EventTrigger>
                                            </i:Interaction.Triggers>
                                        </TextBlock>
                                    </StackPanel>
                                </DataTemplate>
                            </ListBox.ItemTemplate>
                        </ListBox>
                    </Grid>
                </Grid>
                <!--  歌词栏  -->
                <Grid Grid.Column="1">
                    <ListBox
                        Height="{c:Binding ((ScreenHeight*0.65)-30),
                                           Source={x:Static res:CandySoft.Default}}"
                        Background="Transparent"
                        BorderThickness="0"
                        ItemsSource="{Binding LyricResult}"
                        Style="{StaticResource WrapPanelVerticalListBox}">
                        <ListBox.ItemTemplate>
                            <DataTemplate>
                                <TextBlock
                                    Margin="5,5,5,10"
                                    FontSize="16"
                                    Foreground="{DynamicResource SugarThemeWhite}"
                                    Text="{Binding Lyric}"
                                    TextWrapping="WrapWithOverflow" />
                            </DataTemplate>
                        </ListBox.ItemTemplate>
                    </ListBox>
                </Grid>
            </Grid>
            <!--  音量设置  -->
            <SelectiveScrollingGrid
                x:Name="音量设置"
                Width="75"
                Height="220"
                Margin="0,0,40,0"
                HorizontalAlignment="Right"
                VerticalAlignment="Bottom"
                RenderTransformOrigin="0.5,0.5"
                Visibility="Collapsed">
                <SelectiveScrollingGrid.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform />
                        <TranslateTransform />
                    </TransformGroup>
                </SelectiveScrollingGrid.RenderTransform>
                <Rectangle
                    Width="75"
                    Height="215"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Fill="White"
                    RadiusX="14.5"
                    RadiusY="14.5"
                    Stroke="{DynamicResource SugarThemePink}" />
                <Label
                    x:Name="VolumeShow"
                    Width="75"
                    Height="30"
                    Margin="0,185,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    HorizontalContentAlignment="Center"
                    VerticalContentAlignment="Center"
                    Content="100%"
                    FontSize="18" />
                <Slider
                    Margin="27,20,21,35"
                    BorderBrush="Transparent"
                    Focusable="False"
                    Foreground="{DynamicResource SugarThemePink}"
                    Maximum="100"
                    Minimum="1"
                    Orientation="Vertical"
                    SmallChange="1"
                    TickFrequency="5"
                    TickPlacement="BottomRight"
                    ValueChanged="VolChangeEvent"
                    Value="100" />
            </SelectiveScrollingGrid>
            <!--  播放器  -->
            <Grid Grid.Row="1" Margin="10,10,0,0">
                <Rectangle Height="{c:Binding ((ScreenHeight-30)*0.2), Source={x:Static res:CandySoft.Default}}" StrokeThickness="0">
                    <Rectangle.Fill>
                        <SolidColorBrush Opacity="0.4" Color="{DynamicResource SugarThemeWhiteColor}" />
                    </Rectangle.Fill>
                    <Rectangle.Effect>
                        <DropShadowEffect
                            BlurRadius="15"
                            Direction="300"
                            Opacity="0.3" />
                    </Rectangle.Effect>
                </Rectangle>
                <!--  信息栏  -->
                <StackPanel Margin="20,10,0,0">
                    <WrapPanel VerticalAlignment="Center" Orientation="Horizontal">
                        <Label
                            x:Name="当前播放"
                            Content="{Binding SongName}"
                            FontSize="25" />
                        <Label
                            x:Name="歌手"
                            VerticalAlignment="Center"
                            Content="{Binding Songer}"
                            FontSize="16" />

                    </WrapPanel>
                    <Label
                        x:Name="时常"
                        VerticalAlignment="Center"
                        FontFamily="14">
                        <Label.Content>
                            <WrapPanel>
                                <TextBlock Text="{Binding CurrentSpan}" />
                                <TextBlock Text="{Binding Audio.TimeSpan, StringFormat=/{0}}" />
                            </WrapPanel>
                        </Label.Content>
                    </Label>
                    <Slider
                        x:Name="播放条"
                        Width="400"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Center"
                        AutoToolTipPrecision="2"
                        Cursor="Hand"
                        Foreground="{DynamicResource SugarThemePink}"
                        IsEnabled="True"
                        IsMoveToPointEnabled="True"
                        Maximum="{Binding Audio.Seconds}"
                        SmallChange="0.001"
                        SnapsToDevicePixels="True"
                        Style="{DynamicResource PlaySliderStyle}"
                        TickFrequency="0.001"
                        Value="{Binding CurrentSecond}" />
                </StackPanel>
                <!--  控制栏  -->
                <Canvas>
                    <Button
                        x:Name="上一首"
                        Canvas.Left="455"
                        Canvas.Top="45"
                        VerticalAlignment="Bottom"
                        md:ShadowAssist.ShadowDepth="Depth2"
                        s:View.ActionTarget="{Binding DataContext, ElementName=MusicViewName}"
                        Background="{DynamicResource SugarThemeBlue}"
                        BorderBrush="Transparent"
                        Command="{s:Action SkipPreviousAciton}"
                        Style="{DynamicResource MaterialDesignFloatingActionMiniButton}">
                        <md:PackIcon
                            Width="30"
                            Height="30"
                            Kind="SkipPrevious" />
                    </Button>
                    <Button
                        x:Name="下一首"
                        Canvas.Left="605"
                        Canvas.Top="45"
                        VerticalAlignment="Bottom"
                        md:ShadowAssist.ShadowDepth="Depth2"
                        s:View.ActionTarget="{Binding DataContext, ElementName=MusicViewName}"
                        Background="{DynamicResource SugarThemeBlue}"
                        BorderBrush="Transparent"
                        Command="{s:Action SkipNextAciton}"
                        Style="{DynamicResource MaterialDesignFloatingActionMiniButton}">
                        <md:PackIcon
                            Width="30"
                            Height="30"
                            Kind="SkipNext" />
                    </Button>
                    <Button
                        x:Name="暂停"
                        Canvas.Left="520"
                        Canvas.Top="35"
                        Width="60"
                        Height="60"
                        VerticalAlignment="Bottom"
                        md:ShadowAssist.ShadowDepth="Depth2"
                        s:View.ActionTarget="{Binding DataContext, ElementName=MusicViewName}"
                        Background="{DynamicResource SugarThemeBlue}"
                        BorderBrush="Transparent"
                        Command="{s:Action PauseAciton}"
                        Style="{DynamicResource MaterialDesignFloatingActionMiniButton}"
                        Visibility="{Binding NonPlay, Converter={StaticResource ToShow}}">
                        <md:PackIcon
                            Width="40"
                            Height="40"
                            Kind="Pause" />
                    </Button>
                    <Button
                        x:Name="播放"
                        Canvas.Left="520"
                        Canvas.Top="35"
                        Width="60"
                        Height="60"
                        VerticalAlignment="Bottom"
                        md:ShadowAssist.ShadowDepth="Depth2"
                        s:View.ActionTarget="{Binding DataContext, ElementName=MusicViewName}"
                        Background="{DynamicResource SugarThemeBlue}"
                        BorderBrush="Transparent"
                        Command="{s:Action DownPlayAction}"
                        Style="{DynamicResource MaterialDesignFloatingActionMiniButton}"
                        Visibility="{Binding IsPlay, Converter={StaticResource ToShow}}">
                        <md:PackIcon
                            Width="40"
                            Height="40"
                            Kind="play" />
                    </Button>
                </Canvas>
                <!--  模式  -->
                <Canvas>
                    <ListBox
                        x:Name="PlayBoxType"
                        Canvas.Left="80"
                        Canvas.Top="90"
                        Width="{c:Binding (ScreenWidth*0.8),
                                          Source={x:Static res:CandySoft.Default}}"
                        Height="30"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Bottom"
                        Foreground="{DynamicResource SugarThemeWhite}"
                        Style="{StaticResource MaterialDesignToolToggleListBox}">
                        <ListBox.Background>
                            <SolidColorBrush Opacity=".5" Color="{DynamicResource SugarThemeBlueColor}" />
                        </ListBox.Background>
                        <ListBoxItem x:Name="单曲循环" MouseUp="EachEvent">
                            <md:PackIcon Kind="RepeatOnce" />
                            <i:Interaction.Triggers>
                                <i:EventTrigger EventName="Selected">
                                    <i:InvokeCommandAction
                                        s:View.ActionTarget="{Binding DataContext, ElementName=MusicViewName}"
                                        Command="{s:Action ChangeModuleAction}"
                                        CommandParameter="0" />
                                </i:EventTrigger>
                            </i:Interaction.Triggers>
                        </ListBoxItem>
                        <ListBoxItem
                            x:Name="列表循环"
                            IsSelected="True"
                            MouseUp="EachEvent">
                            <md:PackIcon Kind="Repeat" />
                            <i:Interaction.Triggers>
                                <i:EventTrigger EventName="Selected">
                                    <i:InvokeCommandAction
                                        s:View.ActionTarget="{Binding DataContext, ElementName=MusicViewName}"
                                        Command="{s:Action ChangeModuleAction}"
                                        CommandParameter="1" />
                                </i:EventTrigger>
                            </i:Interaction.Triggers>
                        </ListBoxItem>
                    </ListBox>
                    <Button
                        x:Name="音量控制"
                        Canvas.Top="90"
                        Width="50"
                        Height="30"
                        Margin="20,0,0,10"
                        BorderThickness="0"
                        Click="VolumeEvent">
                        <Button.Background>
                            <SolidColorBrush Opacity=".5" Color="{DynamicResource SugarThemeBlueColor}" />
                        </Button.Background>
                        <md:PackIcon Kind="VolumeHigh" />
                    </Button>
                </Canvas>
                <!--  音阶  -->
                <Grid
                    Width="200"
                    Margin="0,0,10,0"
                    HorizontalAlignment="Right">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition />
                        <ColumnDefinition />
                        <ColumnDefinition />
                        <ColumnDefinition />
                        <ColumnDefinition />
                        <ColumnDefinition />
                        <ColumnDefinition />
                        <ColumnDefinition />
                        <ColumnDefinition />
                        <ColumnDefinition />
                        <ColumnDefinition />
                        <ColumnDefinition />
                        <ColumnDefinition />
                        <ColumnDefinition />
                        <ColumnDefinition />
                        <ColumnDefinition />
                    </Grid.ColumnDefinitions>
                    <ProgressBar Style="{StaticResource Channel}" Value="{Binding Channel[0]}" />
                    <ProgressBar
                        Grid.Column="1"
                        Style="{StaticResource Channel}"
                        Value="{Binding Channel[1]}" />
                    <ProgressBar
                        Grid.Column="2"
                        Style="{StaticResource Channel}"
                        Value="{Binding Channel[2]}" />
                    <ProgressBar
                        Grid.Column="3"
                        Style="{StaticResource Channel}"
                        Value="{Binding Channel[3]}" />
                    <ProgressBar
                        Grid.Column="4"
                        Style="{StaticResource Channel}"
                        Value="{Binding Channel[4]}" />
                    <ProgressBar
                        Grid.Column="5"
                        Style="{StaticResource Channel}"
                        Value="{Binding Channel[5]}" />
                    <ProgressBar
                        Grid.Column="6"
                        Style="{StaticResource Channel}"
                        Value="{Binding Channel[6]}" />
                    <ProgressBar
                        Grid.Column="7"
                        Style="{StaticResource Channel}"
                        Value="{Binding Channel[7]}" />
                    <ProgressBar
                        Grid.Column="8"
                        Style="{StaticResource Channel}"
                        Value="{Binding Channel[8]}" />
                    <ProgressBar
                        Grid.Column="9"
                        Style="{StaticResource Channel}"
                        Value="{Binding Channel[9]}" />
                    <ProgressBar
                        Grid.Column="10"
                        Style="{StaticResource Channel}"
                        Value="{Binding Channel[10]}" />
                    <ProgressBar
                        Grid.Column="11"
                        Style="{StaticResource Channel}"
                        Value="{Binding Channel[11]}" />
                    <ProgressBar
                        Grid.Column="12"
                        Style="{StaticResource Channel}"
                        Value="{Binding Channel[12]}" />
                    <ProgressBar
                        Grid.Column="13"
                        Style="{StaticResource Channel}"
                        Value="{Binding Channel[13]}" />
                    <ProgressBar
                        Grid.Column="14"
                        Style="{StaticResource Channel}"
                        Value="{Binding Channel[14]}" />
                    <ProgressBar
                        Grid.Column="15"
                        Style="{StaticResource Channel}"
                        Value="{Binding Channel[15]}" />
                </Grid>
            </Grid>
        </Grid>
    </Grid>
</lib:CandyControl>
